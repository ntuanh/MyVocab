{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "45mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/my_website/static/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/my_website/static/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}